function loadLatestGitVersion() {
  requestXML(GITHUB, (json: JSON[]) =&gt; {
    if (!(json instanceof Array)) return false;
    for (const data of json) {
      if (checkVersion((data as any)[GITHUB.key])) {
        return true;
