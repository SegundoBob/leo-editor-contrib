<t tx="ekr.20211016054411.786">export function getRange(text: string): RangeDef | null {
  const n = text.codePointAt(0);
  for (const range of RANGES) {
    if (n &lt;= range[1]) {
      if (n &gt;= range[0]) {
        return range;
      }
      break;
    }
  }
  return null;
}

