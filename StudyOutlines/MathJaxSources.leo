public childPosition() {
  let child: MmlNode = this;
  let parent = child.parent;
  while (parent &amp;&amp; parent.notParent) {
    child = parent;
    parent = parent.parent;
  }
  if (parent) {
    let i = 0;
    for (const node of parent.childNodes) {
      if (node === child) {
        return i;
      }
      i++;
    }
  }
  return null;
}

