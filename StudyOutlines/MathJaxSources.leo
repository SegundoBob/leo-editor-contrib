public load() {
  if (!this.isLoaded &amp;&amp; !this.isLoading &amp;&amp; !this.noLoad) {
    this.isLoading = true;
    const url = Package.resolvePath(this.name);
    if (CONFIG.require) {
      this.loadCustom(url);
    } else {
      this.loadScript(url);
    }
  }
