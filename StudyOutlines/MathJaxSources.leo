<t tx="ekr.20211016054411.1321">/**
 * @return {string} Get and return a control-sequence name
 */
public GetCS(): string {
  let CS = this.string.slice(this.i).match(/^(([a-z]+) ?|[\uD800-\uDBFF].|.)/i);
  if (CS) {
    this.i += CS[0].length;
    return CS[2] || CS[1];
  } else {
    this.i++;
    return ' ';
