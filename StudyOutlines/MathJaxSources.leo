<t tx="ekr.20211016054411.1202">/**
 * Tests if the node is in the tree spanned by the current root node.
 * @param {MmlNode} node The node to test.
 */
  private inTree(node: MmlNode) {
    while (node &amp;&amp; node !== this.root) {
      node = node.parent;
    }
    return !!node;
  }

}
