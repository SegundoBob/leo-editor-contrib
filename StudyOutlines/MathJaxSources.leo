    protected sanitize(data: {math: MathItem&lt;N, T, D&gt;, document: SafeMathDocument&lt;N, T, D&gt;}) {
      data.math.root = (this as any).parseOptions.root;
      data.document.safe.sanitize(data.math, data.document);
