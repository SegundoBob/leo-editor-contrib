public canStretch(direction: DIRECTION): boolean {
  this.stretch = NOSTRETCH as DD;
  if (this.node.isEmbellished) {
    let core = this.core();
    if (core &amp;&amp; core.node !== this.node) {
      if (core.canStretch(direction)) {
