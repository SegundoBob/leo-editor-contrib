  const rbox = this.getBBox();
  const sbox = surd.getBBox();
  const bbox = base.getBBox();
  const q = this.getPQ(sbox)[1];
  const t = this.font.params.rule_thickness * this.bbox.scale;
  const H = bbox.h + q + t;
  //
