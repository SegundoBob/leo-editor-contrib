public elementsByClass(node: LiteElement, name: string): LiteElement[] {
  let stack = [] as LiteNode[];
  let tags = [] as LiteElement[];
  let n: LiteNode = node;
  while (n) {
    if (n.kind !== '#text' &amp;&amp; n.kind !== '#comment') {
