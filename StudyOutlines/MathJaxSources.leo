public elementById(node: LiteElement, id: string): LiteElement {
  let stack = [] as LiteNode[];
  let n = node as LiteNode;
  while (n) {
    if (n.kind !== '#text' &amp;&amp; n.kind !== '#comment') {
      n = n as LiteElement;
      if (n.attributes['id'] === id) {
