<t tx="ekr.20211016054411.1898">/**
 * @override
 */
public checkExtendedHeight(D: number, C: DelimiterData): number {
  if (C.fullExt) {
    const [extSize, endSize] = C.fullExt;
    const n = Math.ceil(Math.max(0, D - endSize) / extSize);
    D = endSize + n * extSize;
  }
  return D;
}

