 * Stretch the columns to their proper widths
 */
public stretchColumns() {
  for (let i = 0; i &lt; this.numCols; i++) {
    const width = (typeof this.cWidths[i] === 'number' ? this.cWidths[i] as number : null);
    this.stretchColumn(i, width);
