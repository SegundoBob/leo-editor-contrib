<t tx="ekr.20211016054411.1957">/**
 * @override
 */
public extendHD(i: number, H: number[], D: number[], M: number) {
  const d = (M - (H[i] + D[i])) / 2;
  if (d &lt; .00001) return;
  H[i] += d;
  D[i] += d;
}

