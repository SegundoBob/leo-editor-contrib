 * Search for math in an array of strings and return an array of matches.
 *
 * @override
 */
public findMath(strings: string[]) {
  let math: ProtoItem&lt;N, T&gt;[] = [];
  if (this.hasPatterns) {
    for (let i = 0, m = strings.length; i &lt; m; i++) {
