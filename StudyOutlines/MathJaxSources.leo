public dynamicSubmenus() {
  for (const [id, method] of MJContextMenu.DynamicSubmenus) {
    const menu = this.find(id) as Submenu;
    if (!menu) continue;
    const sub = method(this, menu);
    menu.submenu = sub;
    if (sub.items.length) {
      menu.enable();
    } else {
      menu.disable();
    }
  }
}

