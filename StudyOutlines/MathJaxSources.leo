 */
export type MetricMap&lt;N&gt; = Map&lt;N, ExtendedMetrics&gt;;
type MetricDomMap&lt;N&gt; = Map&lt;N, N&gt;;

/**
 * Maps for unknown characters
 */
export type UnknownBBox = {w: number, h: number, d: number};
