  // Populate the IR node
  std::vector&lt;std::string&gt; names;
  std::vector&lt;codon::ir::types::Type *&gt; types;
  for (size_t i = 0, j = 0; i &lt; r-&gt;ast-&gt;args.size(); i++) {
    if (r-&gt;ast-&gt;args[i].status == Param::Normal) {
