                    clone(expr-&gt;expr)));
  }

  // `((a := tuple[0]; expr), (a := tuple[1]; expr), ...)`
  resultExpr = transform(N&lt;StmtExpr&gt;(block, N&lt;TupleExpr&gt;(items)));
