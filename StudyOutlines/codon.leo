    if (val-&gt;getId() == loc-&gt;getId())
      done = true;

    auto killed = getKilled(val);
    if (killed == var-&gt;getId())
      defs.clear();
    auto gen = getGenerated(val);
    if (gen.first == var-&gt;getId())
      defs.insert(gen.second);
  }

  if (defs.find(-1) != defs.end())
    return {};

  return defs;
}

