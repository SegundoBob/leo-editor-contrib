getRequiredGVs(const std::vector&lt;llvm::GlobalValue *&gt; &amp;kernels) {
  llvm::SmallPtrSet&lt;llvm::GlobalValue *, 32&gt; keep;
  for (auto *G : kernels) {
    exploreGV(G, keep);
  }
  return std::vector&lt;llvm::GlobalValue *&gt;(keep.begin(), keep.end());
}

