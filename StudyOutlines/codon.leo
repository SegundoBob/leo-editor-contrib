      auto otherSet = rd.getReachingDefinitions(toVar, toCause);
      for (auto &amp;elem : mySet) {
        if (otherSet.count(elem)) {
          derived = true;
          break;
        }
      }
    }

    if (derived) {
      for (auto *fromVar : fromVars) {
        dset.setDerived(fromVar, toCause);
