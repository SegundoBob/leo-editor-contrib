def inline_me_aggressive_nested_while_finally(n):
    while n != 0:
        try:
            while n != 0:
                if n == 42:
                    n -= 1
                    continue
                try:
                    if n &gt; 0:
                        continue
                    else:
                        break
                finally:
                    return -1
            return -2
        finally:
            return n + 1

