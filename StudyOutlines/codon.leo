# 32-bit float ops

NAN32 = math.nan32
INF32 = math.inf32
NINF32 = -math.inf32


def close32(a: float32, b: float32, epsilon: float32 = 1e-5f32):
    return abs(a - b) &lt;= epsilon


@test
