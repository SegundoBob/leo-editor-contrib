    return {};

  auto *blk = cfg-&gt;getBlock(loc);
  if (!blk)
    return {};
  auto &amp;entry = sets[blk-&gt;getId()];
  auto defs = entry.in[var-&gt;getId()];
  if (blk-&gt;getId() == cfg-&gt;getEntryBlock()-&gt;getId())
    defs.insert(-1);

  auto done = false;
  for (auto *val : *blk) {
