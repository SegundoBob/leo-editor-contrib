      valBool = M-&gt;Nr&lt;TernaryInstr&gt;(valBool, M-&gt;getBool(false), M-&gt;getBool(true));
    }

    auto *newReturn = M-&gt;Nr&lt;ReturnInstr&gt;(M-&gt;getBool(any));
    see(newReturn);
    auto *rep = M-&gt;Nr&lt;IfFlow&gt;(valBool, util::series(newReturn));
    v-&gt;replaceAll(rep);
  }

