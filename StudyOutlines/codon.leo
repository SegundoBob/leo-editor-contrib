std::shared_ptr&lt;std::string&gt; TypeContext::getStaticString(types::TypePtr t) {
  if (auto s = t-&gt;getStatic()) {
    auto r = s-&gt;evaluate();
    if (r.type == StaticValue::STRING)
      return std::make_shared&lt;std::string&gt;(r.getString());
  }
  return nullptr;
}

