      auto *lhsCall = cast&lt;CallInstr&gt;(lhs);
      lhs1 = lhsCall-&gt;front();
      lhs2 = lhsCall-&gt;back();
    } else {
      lhs1 = lhs;
      lhs2 = M-&gt;getInt(1);
