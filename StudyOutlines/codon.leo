std::vector&lt;TypePtr&gt; ClassType::getUnbounds() const {
  std::vector&lt;TypePtr&gt; u;
  for (auto &amp;t : generics)
    if (t.type) {
      auto tu = t.type-&gt;getUnbounds();
      u.insert(u.begin(), tu.begin(), tu.end());
    }
  for (auto &amp;t : hiddenGenerics)
    if (t.type) {
      auto tu = t.type-&gt;getUnbounds();
      u.insert(u.begin(), tu.begin(), tu.end());
    }
  return u;
}

