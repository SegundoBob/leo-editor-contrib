struct InstantiateExpr : Expr {
  ExprPtr typeExpr;
  std::vector&lt;ExprPtr&gt; typeParams;

  InstantiateExpr(ExprPtr typeExpr, std::vector&lt;ExprPtr&gt; typeParams);
