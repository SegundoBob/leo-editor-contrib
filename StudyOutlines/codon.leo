<t tx="ekr.20230509083243.1105">static void print_from_last_dot(seq_str_t s, std::ostringstream &amp;buf) {
  char *p = s.str;
  int64_t n = s.len;

  for (int64_t i = n - 1; i &gt;= 0; i--) {
    if (p[i] == '.') {
      p += (i + 1);
      n -= (i + 1);
      break;
    }
  }

  buf.write(p, (size_t)n);
}

