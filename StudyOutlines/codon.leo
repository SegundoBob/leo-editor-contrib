  // Visit the Functions.
  for (auto &amp;F : M) {
    bool del = deleteStuff == (bool)named.count(&amp;F) &amp;&amp; !F.isDeclaration();
    if (!del) {
      if (F.hasAvailableExternallyLinkage())
