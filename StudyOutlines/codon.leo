  auto *p = *result;
  if (!p-&gt;info.stdlibPath.empty()) {
    cache-&gt;pluginImportPaths.push_back(p-&gt;info.stdlibPath);
  }
  for (auto &amp;kw : p-&gt;dsl-&gt;getExprKeywords()) {
    cache-&gt;customExprStmts[kw.keyword] = kw.callback;
