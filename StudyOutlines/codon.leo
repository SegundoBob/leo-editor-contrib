    global TIMESTAMP, MOVES
    TIMESTAMP += 1
    MOVES += 1
    self.board.zobrist.update(self, color)
    self.color = color
    self.reference = self
    self.ledges = 0
    self.used = True
    for neighbour in self.neighbours:
        neighcolor = neighbour.color
        if neighcolor == EMPTY:
