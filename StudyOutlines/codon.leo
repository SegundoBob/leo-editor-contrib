        @pure
        @llvm
        def _llvm_offsetof(T: type, idx: Static[int], TE: type) -&gt; int:
            %a = alloca {=T}
            %b = getelementptr inbounds {=T}, ptr %a, i64 0, i32 {=idx}
            %base = ptrtoint ptr %a to i64
            %elem = ptrtoint ptr %b to i64
