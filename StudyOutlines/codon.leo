    %1 = fdiv double %0, %other
    ret double %1

@pure
@llvm
def _truediv_int_int(self: int, other: int) -&gt; float:
    %0 = sitofp i64 %self to double
    %1 = sitofp i64 %other to double
    %2 = fdiv double %0, %1
    ret double %2

@pure
@llvm
def _mod_int_float(self: int, other: float) -&gt; float:
