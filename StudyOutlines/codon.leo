    for (auto &amp;k : i.conds)
      cond += fmt::format(" if {}", transform(k));

    s += fmt::format("for {} in {}{}", i.vars-&gt;toString(), i.gen-&gt;toString(), cond);
  }
  result =
      renderExpr(expr, "{{{}: {} {}}}", transform(expr-&gt;key), transform(expr-&gt;expr), s);
