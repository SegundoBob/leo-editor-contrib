    return out &lt;&lt; c.result;
  }

  using CallbackASTVisitor&lt;std::string, std::string&gt;::transform;
  template &lt;typename T&gt; std::string transform(const std::vector&lt;T&gt; &amp;ts) {
    std::vector&lt;std::string&gt; r;
    for (auto &amp;e : ts)
