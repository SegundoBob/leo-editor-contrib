  if (startswith(func-&gt;ast-&gt;args[i].name, "**"))
    kwstarArgIndex = i, score -= 2;
  else if (startswith(func-&gt;ast-&gt;args[i].name, "*"))
    starArgIndex = i, score -= 2;
}

