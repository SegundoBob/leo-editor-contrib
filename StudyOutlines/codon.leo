<t tx="ekr.20230509083243.1035">// Return T if t is of type `Generator[T]`; otherwise just `type(t)`
auto getIterableType = [&amp;](TypePtr t) {
  if (t-&gt;is("Generator")) {
    hasGenerator = true;
    return t-&gt;getClass()-&gt;generics[0].type;
  }
  return t;
