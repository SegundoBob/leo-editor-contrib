        board.move(pos)
        child = node.pos_child[pos]
        if not child:
            child = node.pos_child[pos] = UCTNode()
            child.unexplored = board.useful_moves()
            child.pos = pos
            child.parent = node
            path.append(child)
            break
        path.append(child)
        node = child
