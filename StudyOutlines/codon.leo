<t tx="ekr.20230509083242.708">bool anyMatch(types::Type *type, std::vector&lt;types::Type *&gt; types) {
  if (type) {
    for (auto *t : types) {
      if (t &amp;&amp; t-&gt;getName() == type-&gt;getName())
        return true;
    }
  } else {
    for (auto *t : types) {
      if (!t)
        return true;
    }
  }
  return false;
}
