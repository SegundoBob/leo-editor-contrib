@pure
@llvm
def ctlz(n: UInt[N], N: Static[int]) -&gt; UInt[N]:
    declare i{=N} @llvm.ctlz.i{=N}(i{=N}, i1)
    %0 = call i{=N} @llvm.ctlz.i{=N}(i{=N} %n, i1 true)
