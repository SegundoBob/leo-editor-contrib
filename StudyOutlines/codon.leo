<t tx="ekr.20230509083242.757">  bool isNeverEqualToUnescapedAlloc(llvm::Value *value, llvm::Instruction *ai) {
    using namespace llvm;

    if (isa&lt;ConstantPointerNull&gt;(value))
      return true;
    if (auto *li = dyn_cast&lt;LoadInst&gt;(value))
