<t tx="ekr.20230509083242.666">int typeIdxLookup(const std::string &amp;name) {
  static std::unordered_map&lt;std::string, int&gt; cache;
  static int next = 1000;
  if (name.empty())
    return 0;
  auto it = cache.find(name);
  if (it != cache.end()) {
    return it-&gt;second;
  } else {
    const int myID = next++;
    cache[name] = myID;
    return myID;
  }
}
