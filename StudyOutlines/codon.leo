import operator

class Expr:
    def eval(self):
        return 0

class Const(Expr):
    value: int

    def __init__(self, value):
        self.value = value

    def eval(self):
        return self.value

class BinOp(Expr):
    lhs: Expr
    rhs: Expr

    def __init__(self, lhs, rhs):
        self.lhs = lhs
        self.rhs = rhs

    def eval_from_fn(self, fn):
        return fn(self.lhs.eval(), self.rhs.eval())

class Add(BinOp):
    def eval(self):
        return self.eval_from_fn(operator.add)

class Sub(BinOp):
