  std::vector&lt;types::Type *&gt; members = {getIntType(), unsafeGetPointerType(base)};
  std::vector&lt;std::string&gt; names = {"len", "ptr"};
  return Nr&lt;types::RecordType&gt;(name, members, names);
}

