SEQ_FUNC void seq_print_full(seq_str_t str, FILE *fo) {
  if ((seq_flags &amp; SEQ_FLAG_CAPTURE_OUTPUT) &amp;&amp; (fo == stdout || fo == stderr)) {
    captureLock.lock();
    capture.write(str.str, str.len);
    captureLock.unlock();
  } else {
    fwrite(str.str, 1, (size_t)str.len, fo);
  }
}

