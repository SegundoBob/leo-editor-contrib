    if (name == "_loop_reductions") {
      seqassertn(reductionLocRef &amp;&amp; gtid, "bad visit order in template");
      seqassertn(v-&gt;numArgs() == 1 &amp;&amp; isA&lt;VarValue&gt;(v-&gt;front()),
