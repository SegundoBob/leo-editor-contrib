        N&lt;DotExpr&gt;(NT&lt;InstantiateExpr&gt;(
                       NT&lt;IdExpr&gt;(format("{}{}", TYPE_TUPLE, types.size())), types),
                   "__elemsize__"));
    LOG_REALIZE("[poly] {} : {}", t, *suite);
