  if (from) {
    Expr *e = from.get();
    while (auto d = e-&gt;getDot())
      e = d-&gt;expr.get();
    if (!from-&gt;isId("C") &amp;&amp; !from-&gt;isId("python")) {
      if (!e-&gt;getId())
        E(Error::IMPORT_IDENTIFIER, e);
      if (!args.empty())
        E(Error::IMPORT_FN, args[0]);
      if (ret)
