    triple.getTriple(), cpuStr, featuresStr, options,
    llvm::codegen::getExplicitRelocModel(), llvm::codegen::getExplicitCodeModel(),
    llvm::CodeGenOpt::Aggressive));

M-&gt;setDataLayout(machine-&gt;createDataLayout());
