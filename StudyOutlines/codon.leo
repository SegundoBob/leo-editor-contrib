llvm::Error Compiler::compile() {
  pm-&gt;run(module.get());
  if (codon::getLogger().flags &amp; codon::Logger::FLAG_USER) {
    auto fo = fopen("_dump_ir_opt.sexp", "w");
    fmt::print(fo, "{}\n", *module);
    fclose(fo);
