  for (auto &amp;kw : p-&gt;dsl-&gt;getBlockKeywords()) {
    cache-&gt;customBlockStmts[kw.keyword] = {kw.hasExpr, kw.callback};
  }
  p-&gt;dsl-&gt;addIRPasses(pm.get(), debug);
  return llvm::Error::success();
}

